{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    }
  ],
  "alias": [
    "audio-dashboard-levante.vercel.app",
    "levante-audio-dashboard.vercel.app"
  ],
  "routes": [
    {
      "src": "/main.html",
      "dest": "/main.html"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/main.html"
    }
  ]
} 